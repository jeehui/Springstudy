package com.goodee.ex01.java02;

import java.util.List;
import java.util.Map;
import java.util.Set;

public class Student {
	
	//field
	private List<Integer> scores; //0~100 사이 랜덤 5개 정수
	private Set<String> awards; // 임의의 상장 3개
	private Map<String, String> home; //주소, 전화번호
	
	//constructor
	public Student(List<Integer> scores, Set<String> awards, Map<String, String> home) {
		super();
		this.scores = scores;
		this.awards = awards;
		this.home = home;
	}
	
	
	
	
}
