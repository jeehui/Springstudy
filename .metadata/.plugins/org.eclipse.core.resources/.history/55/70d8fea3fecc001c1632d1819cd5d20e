package com.goodee.ex01.xml01;

//Calculator 기능을 내장한 공학용 계산기
public class EngineerCalculator {
	
	// field
	private int a;
	private int b;
	private Calculator calculator;
	
	//default constructor
	private EngineerCalculator() {
		
	}
	
	//method
	public void add() {
		calculator.add(a, b);
	}
	public void sub() {
		calculator.sub(a, b);
	}
	public void mul() {
		calculator.mul(a, b);
	}
	public void div() {
		calculator.div(a, b);
	}
	public void mod() {
		calculator.mod(a, b);
	}
	
}
