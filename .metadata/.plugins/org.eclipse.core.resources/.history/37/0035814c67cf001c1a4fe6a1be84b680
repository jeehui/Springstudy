package com.goodee.ex01.java03;

public class Gugudan {
	//field
	private Calculator calculator;
	private int begin;
	private int end;
	
	
	

	public Gugudan(Calculator calculator, int begin, int end) {
		super();
		this.calculator = calculator;
		this.begin = begin;
		this.end = end;
	}

	//begin~end 단 출력하기
	public void printGugudan() {
		for(int n = 1; n<= 9; n++) {
			for(int dan = begin; dan<=end; begin++) {
				System.out.println(begin + "x" + end + "=" + calculator.mul(begin, end) + "\t");
			}
			System.out.println();
		}
	}
}
