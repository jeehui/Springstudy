package com.goodee.ex01.java05;

import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;

@Configuration
public class SpringBeanConfig {
	@Bean(name="Gun2")
	public Gun a() {
		Gun gun = new Gun();
		gun.setModel("AK-47");
		gun.setBullet("20");
	}
}
